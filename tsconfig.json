{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "noEmit": true,
    "strict": true,
    "esModuleInterop": true,
    "pretty": true,
    "jsx": "preserve",
    "jsxFactory": "h",
    "baseUrl": ".",
    "paths": {
      "/web_modules/*": [
        "./node_modules/*"
      ]
    }
  }
}
